{
  "identifier": "chrisdl.Sort",
  "name": "Sort",
  "organization": "Chris DiLorenzo",
  "description": "Sort lines by content or length in the editor.",
  "version": "1.3.2",
  "categories": ["commands"],
  "repository": "https://github.com/chrisdl/nova-sort",
  "bugs": "https://github.com/chrisdl/nova-sort/issues",
  "main": "main.js",
  
  "activationEvents": ["*"],
  "entitlements": { },
  
  "commands": {
    "editor": [
      {
        "title": "Sort --> Lines",
        "command": "sort.lines"
      },
      {
        "title": "Sort --> Inline",
        "command": "sort.inline"
      },
      {
        "title": "Sort --> By Line Length",
        "command": "sort.byLineLength"
      },
      {
        "title": "Sort --> By Line Length (reversed)",
        "command": "sort.byLineLengthReversed"
      }
    ]
  }
}
